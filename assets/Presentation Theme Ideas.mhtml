  // Widget to build rating row for each concept
  Widget _buildRatingRow(String concept) {
    return Column(
      crossAxisAlignment: CrossAxisAlignment.start,
      children: [
        Text(
          concept,
          style: TextStyle(fontSize: 18, fontWeight: FontWeight.w600),
        ),
        Row(
          children: [
            Icon(Icons.star_border, color: Colors.deepPurpleAccent),
            Icon(Icons.star_border, color: Colors.deepPurpleAccent),
            Icon(Icons.star_border, color: Colors.deepPurpleAccent),
            Icon(Icons.star_border, color: Colors.deepPurpleAccent),
            Icon(Icons.star_border, color: Colors.deepPurpleAccent),
          ],
        ),
        SizedBox(height: 10),
      ],
    );
  }

  // Widget to build overall session rating (1-5)
  Widget _buildOverallSessionRating() {
    return Row(
      mainAxisAlignment: MainAxisAlignment.center,
      children: [
        Icon(Icons.star_border, size: 40, color: Colors.deepPurpleAccent),
        Icon(Icons.star_border, size: 40, color: Colors.deepPurpleAccent),
        Icon(Icons.star_border, size: 40, color: Colors.deepPurpleAccent),
        Icon(Icons.star_border, size: 40, color: Colors.deepPurpleAccent),
        Icon(Icons.star_border, size: 40, color: Colors.deepPurpleAccent),
      ],
    );
  }

  // Widget to build text input field for feedback
  Widget _buildTextInputField(String hint) {
    return TextField(
      decoration: InputDecoration(
        hintText: hint,
        border: OutlineInputBorder(
          borderRadius: BorderRadius.circular(10),
        ),
        filled: true,
        fillColor: Colors.grey[100],
      ),
      maxLines: 3,
    );
  }

  // Widget to build Yes/No selector for resource helpfulness
  Widget _buildYesNoSelector() {
    return Row(
      mainAxisAlignment: MainAxisAlignment.center,
      children: [
        ElevatedButton(
          onPressed: () {
            // Yes logic
          },
          child: Text('Yes'),
          style: ElevatedButton.styleFrom(
            primary: Colors.deepPurpleAccent,
            padding: EdgeInsets.symmetric(horizontal: 30, vertical: 10),
            shape: RoundedRectangleBorder(
              borderRadius: BorderRadius.circular(15),
            ),
          ),
        ),
        SizedBox(width: 20),
        ElevatedButton(
          onPressed: () {
            // No logic
