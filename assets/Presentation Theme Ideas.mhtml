            // Section: Study Goals
            _buildSectionHeader('Study Goals'),
            SizedBox(height: 10),
            _buildTextInputField('Short-term Goals', Icons.short_text, fals=
e),
            SizedBox(height: 15),
            _buildTextInputField('Long-term Goals', Icons.assessment_outlin=
ed, false),
            SizedBox(height: 30),

            // Section: Notifications
            _buildSectionHeader('Notifications'),
            SizedBox(height: 10),
